-- Redoke Hub
-- Roblox Script with Rayfield UI

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Redoke Hub",
   LoadingTitle = "Redoke Hub Loading",
   LoadingSubtitle = "by Redoke",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "RedokeHub",
      FileName = "RedokeConfig"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Redoke Hub",
      Subtitle = "Key System",
      Note = "No key required",
      FileName = "RedokeKey",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

-- Main Tab
local MainTab = Window:CreateTab("Main", 4483362458)

-- Pet Spawner Section
local PetSection = MainTab:CreateSection("Pet Spawner")

-- Pet Spawner Button
local PetSpawnerButton = MainTab:CreateButton({
   Name = "Pet Spawner",
   Callback = function()
      -- Execute the pet spawner script
      local success, err = pcall(function()
         local Spawner = loadstring(game:HttpGet("https://gitlab.com/darkiedarkie/dark/-/raw/main/Spawner.lua"))()
         Spawner.Load()
      end)
      
      if success then
         Rayfield:Notify({
            Title = "Pet Spawner",
            Content = "Pet Spawner loaded successfully!",
            Duration = 3,
            Image = 4483362458,
            Actions = {}
         })
      else
         Rayfield:Notify({
            Title = "Pet Spawner Error",
            Content = "Failed to load Pet Spawner: " .. tostring(err),
            Duration = 5,
            Image = 4483362458,
            Actions = {}
         })
      end
   end,
})

-- Egg Randomizer Button
local EggRandomizerButton = MainTab:CreateButton({
   Name = "Egg Randomizer",
   Callback = function()
      -- Execute the egg randomizer script
      local success, err = pcall(function()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/Kenri-rec/Grow-A-Garden-Script/refs/heads/main/Egg-Randomizer"))()
      end)
      
      if success then
         Rayfield:Notify({
            Title = "Egg Randomizer",
            Content = "Egg Randomizer loaded successfully!",
            Duration = 3,
            Image = 4483362458,
            Actions = {}
         })
      else
         Rayfield:Notify({
            Title = "Egg Randomizer Error",
            Content = "Failed to load Egg Randomizer: " .. tostring(err),
            Duration = 5,
            Image = 4483362458,
            Actions = {}
         })
      end
   end,
})

-- Initialize the UI
Rayfield:LoadConfiguration()
